{
  "className": "ru.tinkoff.zeppelin.interpreter.python.PythonConditionInterpreter",
  "properties": {
    "python.condition.wait.exit.code": {
      "envName": "PYTHON_CONDITION_WAIT_EXIT_CODE",
      "propertyName": "python.condition.wait.exit.code",
      "defaultValue": "100",
      "currentValue": "100",
      "description": "Exit code when interpreter continue cycling. MUST be > 0 && < 120",
      "type": "number"
    },
    "python.condition.timeout": {
      "envName": "PYTHON_CONDITION_TIMEOUT",
      "propertyName": "python.condition.timeout",
      "defaultValue": "1800",
      "currentValue": "1800",
      "description": "Max execution time of interpreter(sec). MUST be > 0 && < INT_MAX",
      "type": "number"
    },
    "python.condition.cycle.timeout": {
      "envName": "PYTHON_CONDITION_CYCLE_TIMEOUT",
      "propertyName": "python.condition.cycle.timeout",
      "defaultValue": "300",
      "currentValue": "300",
      "description": "Time between condition check(sec). MUST be > 0 && < INT_MAX",
      "type": "number"
    },
    "python.working.dir": {
      "envName": "PYTHON_WORKING_DIR",
      "propertyName": "python.working.dir",
      "defaultValue": "",
      "currentValue": "",
      "description": "Path to temp storage of python interpreter results",
      "type": "textarea"
    },
    "python.jep.library.file": {
      "envName": "PYTHON_JEP_LIBRARY_FILE",
      "propertyName": "python.jep.library.file",
      "defaultValue": "",
      "currentValue": "",
      "description": "Path to JEP *.so library",
      "type": "textarea"
    },
    "python.jep.config.include.paths": {
      "envName": "PYTHON_JEP_CONFIG_INCLUDE_PATHS",
      "propertyName": "python.jep.config.include.paths",
      "defaultValue": "",
      "currentValue": "",
      "description": "Adds a path of directories separated by File.pathSeparator that will be appended to the sub-intepreter's sys.path",
      "type": "textarea"
    },
    "python.jep.config.python.home": {
      "envName": "PYTHON_JEP_CONFIG_PYTHON_HOME",
      "propertyName": "python.jep.config.python.home",
      "defaultValue": "",
      "currentValue": "",
      "description": "Set the python's home location, if not specified then use standard",
      "type": "textarea"
    },
    "python.watchdog.time": {
      "envName": "PYTHON_WATCHDOG_TIME",
      "propertyName": "python.watchdog.time",
      "defaultValue": "-1",
      "currentValue": "-1",
      "description": "Watchdog timeout for external python process (ms)",
      "type": "number"
    },
    "python.subprocess.java.args": {
      "envName": "PYTHON_SUBPROCESS_JAVA_ARGS",
      "propertyName": "python.subprocess.java.args",
      "defaultValue": "",
      "currentValue": "",
      "description": "Additional JVM args(used for inner JVM)",
      "type": "textarea"
    },
    "python.env.cache.folder": {
      "envName": "PYTHON_ENV_CACHE_FOLDER",
      "propertyName": "python.env.cache.folder",
      "defaultValue": "./noteContext",
      "currentValue": "./noteContext",
      "description": "path to cache folder",
      "type": "textarea"
    },
    "python.autoimport": {
      "envName": "PYTHON_AUTOIMPORT",
      "propertyName": "python.autoimport",
      "defaultValue": "",
      "currentValue": "",
      "description": "autoimported modules. Separated by ';'. Example module:name;module2:name2 -> import module as name & import module2 as name2",
      "type": "textarea"
    }
  },
  "editor": {
    "language": "python",
    "editOnDblClick": false,
    "completionSupport": false
  }
}
